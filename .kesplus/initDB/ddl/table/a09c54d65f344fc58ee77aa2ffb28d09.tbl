{"userName":"546508b9842c4e3698a1f2d3ae6fa917","encoding":"0","time":"2025-12-22 23:53:28","sql":"CREATE TABLE \"${schemaName}\".\"user\"(\n\"id\" varchar   NOT NULL ,\n\"module_code\" varchar   NOT NULL ,\n\"dept_id\" varchar   NOT NULL ,\nCONSTRAINT \"user_id_pk\" PRIMARY KEY (id) ENABLE VALIDATE \n)  ;\nALTER TABLE \"${schemaName}\".\"user\" SET TABLESPACE sys_default;\n\n\nCOMMENT ON COLUMN \"${schemaName}\".\"user\".\"id\" IS 'id';\nCOMMENT ON COLUMN \"${schemaName}\".\"user\".\"module_code\" IS '模块代码';\nCOMMENT ON COLUMN \"${schemaName}\".\"user\".\"dept_id\" IS '部门ID';\n\n\n\n\n"}
{"userName":"546508b9842c4e3698a1f2d3ae6fa917","encoding":"0","time":"2025-12-22 23:54:10","sql":"do $$begin IF NOT EXISTS (SELECT 1 FROM \"information_schema\".columns WHERE \"table_schema\"='manage_user' AND \"table_name\" ='user' AND \"column_name\" ='user_id') then \nALTER TABLE \"${schemaName}\".\"user\" ADD COLUMN \"user_id\" integer  NOT NULL  ;\nEND if;end $$\n;\nCREATE SEQUENCE \"${schemaName}\".\"user_user_id\" INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 99999999 CACHE 1;\nALTER TABLE \"${schemaName}\".\"user\" ALTER \"user_id\" SET DEFAULT nextval('manage_user.user_user_id'::regclass);\nCOMMENT ON COLUMN \"${schemaName}\".\"user\".\"user_id\" IS '用户ID';\n"}
