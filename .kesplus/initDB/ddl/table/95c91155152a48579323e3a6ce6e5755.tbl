{"userName":"546508b9842c4e3698a1f2d3ae6fa917","encoding":"0","time":"2025-12-20 22:55:30","sql":"CREATE TABLE \"${schemaName}\".\"material\"(\n\"id\" varchar   NOT NULL ,\n\"module_code\" varchar   NOT NULL ,\n\"dept_id\" varchar   NOT NULL ,\n\"material_id\" integer  AUTO_INCREMENT NOT NULL ,\n\"course_id\" bpchar (4)  NOT NULL ,\n\"title\" varchar (200)  NOT NULL ,\n\"file_id\" varchar (100)  NOT NULL ,\nCONSTRAINT \"material_id_pk\" PRIMARY KEY (id) ENABLE VALIDATE \n)  ;\nALTER TABLE \"${schemaName}\".\"material\" SET TABLESPACE sys_default;\n\n\nCOMMENT ON COLUMN \"${schemaName}\".\"material\".\"id\" IS 'id';\nCOMMENT ON COLUMN \"${schemaName}\".\"material\".\"module_code\" IS '模块代码';\nCOMMENT ON COLUMN \"${schemaName}\".\"material\".\"dept_id\" IS '部门ID';\nCOMMENT ON COLUMN \"${schemaName}\".\"material\".\"material_id\" IS '资料ID';\nCOMMENT ON COLUMN \"${schemaName}\".\"material\".\"course_id\" IS '课程ID';\nCOMMENT ON COLUMN \"${schemaName}\".\"material\".\"title\" IS '标题';\nCOMMENT ON COLUMN \"${schemaName}\".\"material\".\"file_id\" IS '附件';\n\n\n\n\n"}
