{"userName":"546508b9842c4e3698a1f2d3ae6fa917","encoding":"0","time":"2025-12-20 22:49:20","sql":"CREATE TABLE \"${schemaName}\".\"course\"(\n\"id\" varchar   NOT NULL ,\n\"module_code\" varchar   NOT NULL ,\n\"dept_id\" varchar   NOT NULL ,\n\"course_id\" bpchar (4)  NOT NULL ,\n\"name\" varchar (50)  NOT NULL ,\nCONSTRAINT \"course_id_pk\" PRIMARY KEY (id) ENABLE VALIDATE ,\nCONSTRAINT \"course_id\" UNIQUE (course_id) ENABLE VALIDATE\n)  ;\nALTER TABLE \"${schemaName}\".\"course\" SET TABLESPACE sys_default;\n\n\nCOMMENT ON COLUMN \"${schemaName}\".\"course\".\"id\" IS 'id';\nCOMMENT ON COLUMN \"${schemaName}\".\"course\".\"module_code\" IS '模块代码';\nCOMMENT ON COLUMN \"${schemaName}\".\"course\".\"dept_id\" IS '部门ID';\nCOMMENT ON COLUMN \"${schemaName}\".\"course\".\"course_id\" IS '课程ID';\nCOMMENT ON COLUMN \"${schemaName}\".\"course\".\"name\" IS '课程名称';\n\n\n\n\n"}
